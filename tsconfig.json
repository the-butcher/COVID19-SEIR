{
  "compilerOptions": {
    "module": "es6",
    "lib": ["dom", "es2015.promise", "es5", "es6"],
    "target": "es5",
    "noImplicitAny": true,
    "removeComments": true,
    "jsx": "react",
    "jsxFactory": "tsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "isolatedModules": false,
    "experimentalDecorators": true,
    "declaration": false,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    //"strictNullChecks": true,
    //"outDir": "./dist/",
    "outDir": "src/main/webapp/js",
    "sourceMap": true,
    "noEmitOnError": false,
    //"noUnusedLocals": true,
    "importHelpers": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "allowJs": true,
    "types": [
      "node",
      "jest"
    ],
    "typeRoots": [

    ]
  },
  "include": [
    "src/**/*"
  ]
}
